<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Companyrule_Attach-Mapper">
	<select id="selectAttachByCompanyrule_attach_number" resultType="companyrule_attach">
		select * 
		from companyrule_attach 
		where companyrule_attach_number = #{companyrule_attach_number}
	</select>	
	
	<select id="selectCompanyrule_AttachByAno" resultType="companyrule_attach">
		select *
		from companyrule_attach
		where ano=#{ano}
	</select>
	
	<update id="insertCompanyrule_Attach">
		insert into 
		companyrule_attach (ano, uploadpath, filename, filetype, companyrule_attach_number, companyrule_attach_attacher)
		values (companyrule_attach_seq.nextval, #{uploadPath}, #{fileName},#{fileType}, 
				#{companyrule_attach_number},#{companyrule_attach_attacher} )
	</update>
	
	<update id="deleteCompanyrule_Attach">
		delete from companyrule_attach where ano = #{ano}
	</update>
	
	<update id="deleteAllCompanyrule_Attach">
		delete 
		from companyrule_attach 
		where companyrule_attach_number = #{companyrule_attach_number}
	</update>
</mapper>







